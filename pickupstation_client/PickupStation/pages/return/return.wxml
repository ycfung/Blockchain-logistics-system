<!--pages/return/return.wxml-->
<view class="page">
  <view class="page__hd">
      <view class="page__title">逾期退还</view>
      <view class="page__desc">将货物发回原地址</view>
  </view>
  <view class="page__bd page__bd_spacing">
    <view class="page__ft">
    <view class="weui-cells weui-cells_after-title">
    <view class="weui-cell ">
      <input name="pNumber" class="weui-input" placeholder="请输入货单号" bindinput="input"/>
    </view>
    </view>
    <view class="weui-btn-area">
            <button class="weui-btn" type="primary"  bindtap="return">确定</button>
    </view>
    <view class="weui-btn-area">
            <button class="weui-btn"  bindtap="history">查看退还记录</button>
    </view>
    </view>
  </view>
</view>
